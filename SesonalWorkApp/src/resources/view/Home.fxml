<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.ExitController">
   <children>
      <BorderPane layoutX="200.0" layoutY="100.0" prefHeight="142.0" prefWidth="308.0">
         <center>
            <ImageView fitHeight="136.0" fitWidth="203.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../icon/icon.png" />
               </image>
            </ImageView>
         </center>
         <top>
            <Label minWidth="140.0" prefHeight="15.0" prefWidth="94.0" text="SEASONAL WORKER" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
      <VBox layoutX="1.0" prefHeight="400.0" prefWidth="113.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="113.0" text="Search Record" translateY="50.0" />
            <Button fx:id="insertRecord" onAction="#insertHandler" mnemonicParsing="false" prefHeight="40.0" prefWidth="113.0" text="Insert Record" translateY="50.0" />
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="113.0" text="Update Record" translateY="50.0" />
            <Button fx:id="logOut" onAction="#logOutHandler" mnemonicParsing="false" prefHeight="40.0" prefWidth="113.0" text="Log-Out" translateY="140.0" />
            <Button fx:id="exit" onAction="#exitHandler" prefHeight="40.0" prefWidth="113.0" text="Exit" translateY="140.0" />
         </children>
      </VBox>
      <TitledPane animated="false" layoutX="-290.0" layoutY="-169.0" text="untitled" />
   </children>
</AnchorPane>
