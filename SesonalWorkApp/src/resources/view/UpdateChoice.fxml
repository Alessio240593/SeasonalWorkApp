                  <VBox prefHeight="213.0" prefWidth="665.0" translateY="50.0">
                     <children>
                          <Label prefHeight="65.0" prefWidth="472.0" styleClass="title" text="Select Worker" translateX="5.0" translateY="-30.0">
                              <font>
                                  <Font size="25.0" />
                              </font>
                        </Label>
                              <HBox prefHeight="29.0" prefWidth="550.0" spacing="72.0">
                                  <children>
                                      <Label text="Worker id ">
                                          <padding>
                                              <Insets left="10.0" />
                                          </padding>
                                      </Label>
                              <ChoiceBox fx:id="workerId" prefWidth="150.0" translateX="-40.0">
                                  <items>
                                      <FXCollections fx:factory="observableArrayList">
                                </FXCollections>
                                  </items>
                              </ChoiceBox>
                                  </children>
                                  <opaqueInsets>
                                      <Insets left="10.0" />
                                  </opaqueInsets>
                                  <padding>
                                      <Insets bottom="20.0" />
                                  </padding>
                              </HBox>
                     </children>
                  </VBox>
                  <TableView fx:id="table" minWidth="230.0" prefHeight="65.0" prefWidth="665.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="Name">
                          <cellValueFactory>
                              <PropertyValueFactory property="name" />
                          </cellValueFactory>
                      </TableColumn>
                      <TableColumn prefWidth="75.0" text="Surname">
                          <cellValueFactory>
                              <PropertyValueFactory property="surname" />
                          </cellValueFactory>
                      </TableColumn>
                        <TableColumn prefWidth="185.0" text="Email">
                            <cellValueFactory>
                                <PropertyValueFactory property="email" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="129.0" text="Cell">
                            <cellValueFactory>
                                <PropertyValueFactory property="cell" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="84.0" text="BirthDate">
                            <cellValueFactory>
                                <PropertyValueFactory property="birthDate" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="144.0" text="Birth place">
                                <cellValueFactory>
                                    <PropertyValueFactory property="birthPlace" />
                                </cellValueFactory>
                        </TableColumn>
                    </columns>
                  </TableView>
                        <HBox prefHeight="73.0" prefWidth="586.0" spacing="18.0" translateY="-10.0">
                            <children>
                                <Button fx:id="nextUpdate" mnemonicParsing="false" onAction="#nextUpdateHandler" prefHeight="32.0" prefWidth="124.0" text="Next" />
                            </children>
                            <VBox.margin>
                                <Insets top="30.0" />
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>

    <stylesheets>
        <URL value="@../css/InsertExp.css" />
    </stylesheets>

</AnchorPane>
